{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Grpc.AspNetCore.Server.ServerCallHandler": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Urls": "https://*:5005",
  "TransportOptions": {
    "UseMax4096Bytes": false,
    "EnableDataEncrypt": false
  }
}